{
  "projectType": "application",
  "sourceRoot": "apps/dpt-2/src",
  "prefix": "dpt",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/dpt-2",
        "index": "apps/dpt-2/src/index.html",
        "main": "apps/dpt-2/src/main.ts",
        "polyfills": "apps/dpt-2/src/polyfills.ts",
        "tsConfig": "apps/dpt-2/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": ["apps/dpt-2/src/favicon.ico", "apps/dpt-2/src/assets"],
        "styles": [
          "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
          "apps/dpt-2/src/styles.scss",
          "node_modules/bootstrap/dist/css/bootstrap.css"
        ],
        "scripts": ["node_modules/bootstrap/dist/js/bootstrap.bundle.min.js"]
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/dpt-2/src/environments/environment.ts",
              "with": "apps/dpt-2/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "dpt-2:build:production"
        },
        "development": {
          "browserTarget": "dpt-2:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "dpt-2:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["apps/dpt-2/**/*.ts", "apps/dpt-2/**/*.html"]
      }
    }
  },
  "tags": []
}
